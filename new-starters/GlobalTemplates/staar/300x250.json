{
    "template": [{
            "t": "s",
            "s": "/*** PLEASE DO NOT AMEND THIS NORMALISE CSS ***/ html{font-family:sans-serif;line-height:1.15;-ms-text-size-adjust:100%;-webkit-text-size-adjust:100%}body{margin:0}article,aside,footer,header,nav,section{display:block}h1{font-size:2em;margin:.67em 0}figcaption,figure,main{display:block}figure{margin:1em 40px}hr{box-sizing:content-box;height:0;overflow:visible}pre{font-family:monospace,monospace;font-size:1em}a{background-color:transparent;-webkit-text-decoration-skip:objects}a:active,a:hover{outline-width:0}abbr[title]{border-bottom:0;text-decoration:underline;text-decoration:underline dotted}b,strong{font-weight:inherit}b,strong{font-weight:bolder}code,kbd,samp{font-family:monospace,monospace;font-size:1em}dfn{font-style:italic}mark{background-color:#ff0;color:#000}small{font-size:80%}sub,sup{font-size:75%;line-height:0;position:relative;vertical-align:baseline}sub{bottom:-0.25em}sup{top:-0.5em}audio,video{display:inline-block}audio:not([controls]){display:none;height:0}img{border-style:none}svg:not(:root){overflow:hidden}button,input,optgroup,select,textarea{font-family:sans-serif;font-size:100%;line-height:1.15;margin:0}button,input{overflow:visible}button,select{text-transform:none}button,html [type='button'],[type='reset'],[type='submit']{-webkit-appearance:button}button::-moz-focus-inner,[type='button']::-moz-focus-inner,[type='reset']::-moz-focus-inner,[type='submit']::-moz-focus-inner{border-style:none;padding:0}button:-moz-focusring,[type='button']:-moz-focusring,[type='reset']:-moz-focusring,[type='submit']:-moz-focusring{outline:1px dotted ButtonText}fieldset{border:1px solid silver;margin:0 2px;padding:.35em .625em .75em}legend{box-sizing:border-box;color:inherit;display:table;max-width:100%;padding:0;white-space:normal}progress{display:inline-block;vertical-align:baseline}textarea{overflow:auto}[type='checkbox'],[type='radio']{box-sizing:border-box;padding:0}[type='number']::-webkit-inner-spin-button,[type='number']::-webkit-outer-spin-button{height:auto}[type='search']{-webkit-appearance:textfield;outline-offset:-2px}[type='search']::-webkit-search-cancel-button,[type='search']::-webkit-search-decoration{-webkit-appearance:none}::-webkit-file-upload-button{-webkit-appearance:button;font:inherit}details,menu{display:block}summary{display:list-item}canvas{display:inline-block}template{display:none}[hidden]{display:none}.dynamicAdvertContainer{display:none;position:absolute;width:100%;height:100%;border:1px solid black;box-sizing:border-box;background:#EEE;overflow:hidden;line-height:1.15}:focus{outline:0}::-moz-focus-inner{border:0}"
        },
        {
            "t": "s",
            "s": ".dynamicAdvertContainer{cursor:pointer;line-height:1;width:300px;height:250px;}.template-set-frame{position:absolute; top:0px; left:0px; width:100%; height:100%;}"
        },
        {
            "t": "s",
            "s": ".bg,.frame-bg{z-index:1;position:absolute}.bg img,.frame-bg img{max-width:100%}.text-item{position:absolute;z-index:3}.cta{color:#fff}.staar-frame{opacity:0}.staar-frame.dev{opacity:1!important}.staar-start-frame{opacity:1}.staar-start-frame.animate{opacity:0}.staar-middle-frame.animate{animation:animateFrameIn 1s forwards}.staar-end-frame.animate{animation:animateFrameIn 1s forwards}@keyframes animateFrameIn{from{opacity:0}to{opacity:1}}"
        },
        {
            "t": "j",
            "s": ""
        },
        {
            "t": "e",
            "c": "bg",
            "h": "<img src='$BackgroundImage' />"
        },
        {
            "t": "e",
            "c": "main-heading text-item",
            "h": "$mainHeadingCopy",
            "s": "font-size:$copySizeMainHeadingpx;color:$copyColorMainHeading;left:$positioningHorizontalMainHeadingpx;top:$positioningVerticalMainHeadingpx;text-align:$TextAlignMainCopy"
        },
        {
            "t": "e",
            "c": "sub-heading text-item",
            "h": "$subHeadingCopy",
            "s": "font-size:$copySizeSubHeadingpx;color:$copyColourSubHeading;left:$positioningHoriziontialSubHeadingpx;top:$positioningVerticalSubHeadingpx;text-align:$TextAlignSubCopy"
        }
    ],
    "config": [{
            "target": "$BackgroundImage",
            "name": "Background Image",
            "type": "file",
            "groupName": "Background",
            "groupType": "image",
            "initialValue": "https://s0.2mdn.net/creatives/assets/1951882/transparent.png"
        },
        {
            "target": "$mainHeadingCopy",
            "name": "Main Copy",
            "type": "text",
            "groupName": "Main Heading Copy",
            "groupType": "text",
            "initialValue": "Initial Test Copy",
            "properties": {
                "value": "Initial Test Copy",
                "characterLimit": 40
            }
        },
        {
            "target": "$copyColorMainHeading",
            "name": "Copy Colour",
            "type": "radio",
            "groupName": "Main Heading Copy",
            "groupType": "select",
            "initialValue": "white",
            "properties": {
                "value": "white"
            },
            "options": [
                "black",
                "white"
            ],
            "labels": [
                "Black <span style='background-color:black;'></span>",
                "White <span style='background-color:white;'></span>"
            ]
        },
        {
            "target": "$copySizeMainHeading",
            "name": "Copy Size",
            "type": "number",
            "groupName": "Main Heading Copy",
            "groupType": "number",
            "initialValue": 20,
            "properties": {
                "min": 1,
                "max": 28,
                "step": 1,
                "value": 20
            }
        },
        {
            "target": "$positioningHorizontalMainHeading",
            "name": "Copy Position Horizontal",
            "type": "number",
            "groupName": "Main Heading Copy",
            "groupType": "number",
            "initialValue": 30,
            "properties": {
                "min": 0,
                "max": 300,
                "step": 1,
                "value": 0
            }
        },
        {
            "target": "$positioningVerticalMainHeading",
            "name": "Copy Position Vertical",
            "type": "number",
            "groupName": "Main Heading Copy",
            "groupType": "number",
            "initialValue": 30,
            "properties": {
                "min": 0,
                "max": 250,
                "step": 1,
                "value": 0
            }
        },
        {
            "target": "$subHeadingCopy",
            "name": "Copy Text",
            "type": "text",
            "groupName": "Sub Heading Copy",
            "groupType": "text",
            "initialValue": "Initial Sub heading",
            "properties": {
                "value": "Initial Sub heading",
                "characterLimit": 40
            }
        },
        {
            "target": "$copySizeSubHeading",
            "name": "Copy Size",
            "type": "number",
            "groupName": "Sub Heading Copy",
            "groupType": "number",
            "initialValue": 20,
            "properties": {
                "min": 1,
                "max": 30,
                "step": 1,
                "value": 20
            }
        },
        {
            "target": "$copyColourSubHeading",
            "name": "Copy Colour",
            "type": "radio",
            "groupName": "Sub Heading Copy",
            "groupType": "select",
            "initialValue": "white",
            "properties": {
                "value": "white"
            },
            "options": [
                "black",
                "white"
            ],
            "labels": [
                "Black <span style='background-color:black;'></span>",
                "White <span style='background-color:white;'></span>"
            ]
        },
        {
            "target": "$positioningHoriziontialSubHeading",
            "name": "Copy Positioning X",
            "type": "number",
            "groupName": "Sub Heading Copy",
            "groupType": "number",
            "initialValue": 50,
            "properties": {
                "min": 0,
                "max": 300,
                "step": 1,
                "value": 0
            }
        },
        {
            "target": "$positioningVerticalSubHeading",
            "name": "Copy Positioning Y",
            "type": "number",
            "groupName": "Sub Heading Copy",
            "groupType": "number",
            "initialValue": 40,
            "properties": {
                "min": 0,
                "max": 250,
                "step": 1,
                "value": 0
            }
        },
        {
            "target": "$TextAlignMainCopy",
            "name": "Copy Text Alignment",
            "type": "radio",
            "groupName": "Main Heading Copy",
            "groupType": "select",
            "initialValue": "left",
            "properties": {
                "value": "left"
            },
            "options": [
                "left",
                "center",
                "right"
            ],
            "labels": [
                "<span style='text-align:left;width:90%;height:20px;border:none;'>Left</span>",
                "<span style='text-align:center;width:90%;height:20px;border:none;width:90%;height:20px;border:none;'>Center</span>",
                "<span style='text-align:right;width:90%;height:20px;border:none;'>Right</span>"
            ]
        },
        {
            "target": "$TextAlignSubCopy",
            "name": "Copy Text Alignment",
            "type": "radio",
            "groupName": "Sub Heading Copy",
            "groupType": "select",
            "initialValue": "left",
            "properties": {
                "value": "left"
            },
            "options": [
                "left",
                "center",
                "right"
            ],
            "labels": [
                "<span style='text-align:left;width:90%;height:20px;border:none;'>Left</span>",
                "<span style='text-align:center;width:90%;height:20px;border:none;width:90%;height:20px;border:none;'>Center</span>",
                "<span style='text-align:right;width:90%;height:20px;border:none;'>Right</span>"
            ]
        }
    ]
}