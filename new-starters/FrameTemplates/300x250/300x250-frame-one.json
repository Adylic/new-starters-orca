{
  "template": [
    {
      "t": "f",
      "c": "frame-one qb-frame",
      "d": 5000,
      "j": {
        "s": ""
      },
      "e": [
        {
          "t": "e",
          "c": "bg-image",
          "h": "<img src='$backgroundImage' />"
        },
        {
          "t": "e",
          "c": "quickbooks-main-heading $animationMainHeading",
          "h": "$mainHeadingCopy",
          "s": "font-size:$copySizeMainHeadingpx;color:$copyColorMainHeading;left:$positioningHoriziontialMainHeadingpx;top:$positioningVerticalMainHeadingpx"
        },
        {
          "t": "e",
          "c": "quickbooks-sub-heading $SubAnimationHeading",
          "h": "$subHeadingCopy",
          "s": "font-size:$copySizeSubHeadingpx;color:$copyColourSubHeading;left:$positioningHoriziontialSubHeadingpx;top:$positioningVerticalSubHeadingpx"
        },
        {
          "t": "e",
          "c": "cta $CtaAnimation",
          "h": "$cta"
        }
      ]
    }
  ],
  "config": [
    {
      "target": "$backgroundImage",
      "name": "Background Image",
      "type": "file",
      "groupName": "backgrounds",
      "groupType": "image",
      "initialValue": "https://s0.2mdn.net/creatives/assets/4211898/lwf-04121-1.png"
